{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/libc",
                "${workspaceFolder}/build/*/include"
            ],
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "linux-clang-x86",
            "compilerPath": "",
            "configurationProvider": "ms-vscode.makefile-tools",
            "browse": {
                "path": [
                    "${workspaceFolder}/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [
                // see ``VERSIONS``
                "MQ_J=0",
                "MQ_U=1",
                "MQ_E=2",
                "CE_J=3",
                "CE_U=4",
                "CE_E=5",
                "VERSION=MQ_E",
                "DOLPHIN_REV=2003"
            ]
        }
    ],
    "version": 4
}
